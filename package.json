{
  "name": "my-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "rimraf dist/ && concurrently --raw \"vite\" \"cross-env ELEVENTY_ENV=development eleventy --serve --incremental\"",
    "build": "tsc && vite build && cross-env ELEVENTY_ENV=production eleventy && rimraf dist/manifest.json"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@wordpress/browserslist-config": "^4.1.2",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "postcss-hover-media-feature": "^1.0.2",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-css-order": "^1.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "vite": "^2.9.1"
  },
  "engines": {
    "node": "16"
